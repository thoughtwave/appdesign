		"meta_title": "Meteor Generator",
		"theme": "bootswatch-cyborg-darkly",
		"footer_text": "Meteor Kitchen &copy; 2016 Petar KorponaiÄ‡ All Rights Reserved",
		"roles": [
			"admin",
			"user",
			"blocked"
		],
		"default_role": "user",
		"collections": [
			{
				"name": "metadata",
				"type": "collection",
				"roles_allowed_to_update": [
					"admin"
				],
				"roles_allowed_to_delete": [
					"admin"
				]
			},
			{
				"name": "applications",
				"type": "collection",
				"fields": [
					{
						"name": "name",
						"title": "Application name",
						"type": "string",
						"required": true
					},
					{
						"name": "initialTemplate",
						"title": "Initial template",
						"type": "string",
						"input": "radio",
						"input_items": [
							{
								"value": "minimal",
								"title": "Minimal: No user account system"
							},
							{
								"value": "accounts",
								"title": "Minimal: With user account system"
							},
							{
								"value": "admin",
								"title": "Example: Admin"
							},
							{
								"value": "dataview",
								"title": "Example: Dataview"
							},
							{
								"value": "invoices",
								"title": "Example: Invoices"
							},
							{
								"value": "photosharing",
								"title": "Example: Photosharing"
							},
							{
								"value": "iot",
								"title": "Example: IoT - sensors"
							},
							{
								"value": "geiger",
								"title": "Example: IoT - geiger"
							}
						],
						"show_in_dataview": false,
						"show_in_update_form": false,
						"show_in_read_only_form": false
					},
					{
						"name": "githubRepo",
						"title": "GitHub Repository",
						"type": "string",
						"show_in_dataview": false,
						"show_in_insert_form": false,
						"show_in_update_form": false,
						"show_in_read_only_form": false
					}
				],
				"owner_field": "ownerId",
				"roles_allowed_to_read": [
					"owner"
				],
				"roles_allowed_to_insert": [
					"admin",
					"user"
				],
				"roles_allowed_to_update": [
					"owner"
				],
				"roles_allowed_to_delete": [
					"owner"
				],
				"before_insert_source_file": "files/applications_before_insert.js"
			}
		],
		"queries": [
			{
				"name": "metadata",
				"collection": "metadata",
				"find_one": true,
				"filter": "{}",
				"options": "{\"sort\":{\"version\":-1}}"
			},
			{
				"name": "applications",
				"collection": "applications",
				"filter": "{}",
				"options": "{\"sort\":{\"name\":1}}"
			},
			{
				"name": "applications_empty",
				"collection": "applications",
				"find_one": true,
				"filter": "{\"_id\":null}",
				"options": "{}"
			},
			{
				"name": "application",
				"collection": "applications",
				"find_one": true,
				"filter": "{\"_id\":\":applicationId\"}",
				"options": "{}",
				"related_queries": [
					{
						"name": "metadata"
					}
				]
			},
			{
				"name": "admin_users",
				"collection": "users",
				"filter": "{}",
				"options": "{}"
			},
			{
				"name": "admin_user",
				"collection": "users",
				"find_one": true,
				"filter": "{\"_id\":\":userId\"}",
				"options": "{}"
			},
			{
				"name": "users_null",
				"collection": "users",
				"find_one": true,
				"filter": "{\"_id\":null}",
				"options": "{}"
			},
			{
				"name": "current_user_data",
				"collection": "users",
				"find_one": true,
				"filter": "{\"_id\":\"Meteor.userId()\"}",
				"options": "{}"
			}
		],
