							"js": "Template.TEMPLATE_NAME.rendered = function() { Meteor.call(\"userCount\", function(e, r) { if(e) pageSession.set(\"userCount\", e.message); else pageSession.set(\"userCount\", r); }); Meteor.call(\"appCount\", function(e, r) { if(e) pageSession.set(\"appCount\", e.message); else pageSession.set(\"appCount\", r); }); }; Template.TEMPLATE_NAME.helpers({ userCount: function() { return pageSession.get(\"userCount\"); }, appCount: function() { return pageSession.get(\"appCount\"); } });",
